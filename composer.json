{
  "name": "chasegiunta/craft-inertia",
  "type": "craft-plugin",
  "license": "mit",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "chasegiunta\\inertia\\": "src/"
    }
  },
  "extra": {
    "handle": "inertia",
    "name": "Inertia",
    "developer": "chasegiunta",
    "documentationUrl": ""
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
